<% @translate_orders.each do |translate_order| %>
  <%= link_to translate_order.title, translate_order_path(translate_order) %> <br>
  <%= translate_order.description %> <br>
  <%= translate_order.price %> <br>
  <%= translate_order.source_language.name %> <br>
  <%= translate_order.target_language.name %> <br><br>
  <%= link_to translate_order.user.email, profile_path(translate_order.user.profile) %> <br>
<% end %>